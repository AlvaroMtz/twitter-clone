<div id="container">
    <h2>Hi @
        <%= user.username %>!</h2>
    <div id="container">
        <a href="/tweets/new">New tweet</a><br>
        <a href="/logout">Logout</a><br>
        <a href="/tweets">timeline</a><br>
        <h2>@
            <%= username %> tweets</h2>
        <% tweets.forEach(function(tweet) { %>
            <div class="tweet-container">
                <p>
                    <%= tweet.tweet %>
                </p>
                <p class="date">
                    <%= moment(tweet.created_at).format("LL, LTS") %>
                </p>
            </div>
            <% }) %>
    </div>
</div>